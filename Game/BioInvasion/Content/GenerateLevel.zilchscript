class GenerateLevel : ZilchComponent
{
  [Property]
  var Room1 : Archetype = Archetype.Room1;
  [Property]
  var Room2 : Archetype = Archetype.Room2;
  [Property]
  var LevelValue : Integer = 2;
  function Initialize(init : CogInitializer)
  {
    var room1 = this.Space.CreateAtPosition(this.Room1, Real3(0, 0, 0));
    var room1ExitPos = room1.FindChildByName("Exit").Transform.WorldTranslation;
    this.Space.CreateAtPosition(this.Room2, room1ExitPos);
    
    
  }
}
